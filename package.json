{
  "name": "tiny-ding",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "webpack --config ./webpack/webpack.config.base.js",
    "start": "npm run build && cross-env NODE_ENV=production electron .",
    "dev:main": "cross-env NODE_ENV=development electron .",
    "dev:render": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.config.dev.js",
    "dev:server": "npm run build:main && concurrently \"npm run dev:render\" \"npm run dev:main\"",
    "dev:file": "concurrently \"npm run build:main\" \"npm run build:render\" && cross-env NODE_ENV=production electron .",
    "build:main": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.main.js",
    "build:render": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "dll": "webpack --config ./webpack/webpack.config.dll.js",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build:render"
  },
  "keywords": [
    "electron",
    "dingtalk",
    "chat",
    "im"
  ],
  "author": "",
  "license": "ISC",
