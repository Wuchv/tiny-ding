    "dev:preload": "cross-env NODE_ENV=development webpack --config ./app/webpack/webpack.config.preload.js",
    "dev:server": "npm run dev:preload && concurrently \"npm run dev:main\" \"npm run dev:render\" \"gulp watch:electron\"",
    "dev:file": "concurrently \"npm run build:main\" \"npm run build:render\" && cross-env NODE_ENV=production electron .",
    "build:main": "cross-env NODE_ENV=production webpack --config ./app/webpack/webpack.config.main.js",
    "build:render": "cross-env NODE_ENV=production webpack --config ./src/webpack/webpack.config.prod.js",
    "dll": "webpack --config ./src/webpack/webpack.config.dll.js",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build:render"
  },
